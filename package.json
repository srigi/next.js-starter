{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "export": "next export",
    "knex": "TS_NODE_COMPILER_OPTIONS='{ \"module\":\"commonjs\" }' knex",
    "lint": "eslint '**/*.{ts,tsx}'",
    "lt": "yarn lint && yarn tsc",
    "start": "next start",
    "tsc": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "post-checkout": "./scripts/write-git-sha .env.local GIT_REV",
      "post-commit": "./scripts/write-git-sha .env.local GIT_REV",
      "post-merge": "./scripts/write-git-sha .env.local GIT_REV"
    }
  },
  "engines": {
    "node": ">=14.15 || >=12.19",
    "yarn": "^1.22"
  },
  "dependencies": {
    "@sentry/browser": "^5.26.0",
    "@sentry/integrations": "^5.26.0",
    "@sentry/node": "^5.26.0",
    "@sentry/webpack-plugin": "^1.13.0",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "argon2": "^0.27.0",
    "bootstrap": "^4.5.3",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "knex": "^0.21.12",
    "light-date": "^1.2.0",
    "next": "~10.0.0",
    "next-connect": "^0.9.0",
    "pg": "^8.4.2",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^17.0.1",
